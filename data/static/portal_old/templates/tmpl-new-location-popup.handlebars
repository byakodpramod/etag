<form id="submit_location" onsubmit = "form_submit_insert('submit_location'); return false;">

        <div class="form-group" style="display:none">
          <label class="control-label" for="url">URL</label>
          <input type="text" class="form-control" name="url" id="url" value="{{ url }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="reader">Reader</label>
          <input type="text" class="form-control" name="reader" id="reader" value="{{ reader }}" readonly>
        </div>

        <div class="form-group">
          <label class="control-label" for="start_timestamp">Start Timestamp</label>
          <input type="text" class="form-control" name="start_timestamp" id="start_timestamp" value="{{ start_timestamp }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="end_timestamp">End Timestamp</label>
          <input type="text" class="form-control" name="end_timestamp" id="end_timestamp" value="{{ end_timestamp }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="longitude">Longitude</label>
          <input type="number" class="form-control" name="longitude" id="longitude" value={{ longitude }}>
        </div>

        <div class="form-group">
          <label class="control-label" for="latitude">Latitude</label>
          <input type="number" class="form-control" name="latitude" id="latitude" value={{ latitude }}>
        </div>

        <div class="checkbox">
        <label class="control-label" for="active">
          <input type="checkbox" name="active" id="active" value={{ active }} checked> Active
        </label>
      </div>

        <div class="form-group">
          <div class="sumbit">
            <button class="btn btn-primary" id="submitButton" value="PUT">Submit</button>
          </div>
        </div>
</form>
