<form id="submit_animal" onsubmit = "form_submit('submit_animal'); return false;">

        <div class="form-group" style="display:none">
          <label class="control-label" for="url">URL</label>
          <input type="text" class="form-control" name="url" id="url" value="{{ url }}">
        </div>

        <div class="form-group" style="display:none">
          <label class="control-label" for="tag">Tag</label>
          <input type="text" class="form-control" name="tag" id="tag" value="{{ tag }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="name">Name</label>
          <input type="text" class="form-control" name="name" id="name" value="{{ name }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="description">Description</label>
          <input type="text" class="form-control" name="description" id="description" value="{{ description }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="start_timestamp">Start Timestamp</label>
          <input type="text" class="form-control" name="start_timestamp" id="start_timestamp" value="{{ start_timestamp }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="end_timestamp">End Timestamp</label>
          <input type="text" class="form-control" name="end_timestamp" id="end_timestamp" value="{{ end_timestamp }}">
        </div>

        <div class="form-group">
          <label class="control-label" for="field_data">Field Data</label>
          <input type="text" class="form-control" name="field_data" id="field_data" value="{{ field_data }}">
        </div>

        <div class="form-group">
          <div class="sumbit">
            <button class="btn btn-primary" id="submitButton" value="PUT">Submit</button>
          </div>
        </div>
</form>
